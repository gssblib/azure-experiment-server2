<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE document SYSTEM "rml.dtd">

<document>
  <pageInfo pageSize="{{page_size}}" />
  <pageDrawing>

    <setFont name="Times-Bold" size="16" />
    <drawCenteredString
       x="{{page_width * 72 / 2}}"
       y="{{page_height * 72 / 2 - 8}}">
      Barcode on inside
    </drawCenteredString>

  </pageDrawing>
</document>
